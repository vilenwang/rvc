{
  "components/common/quick-search": "{{{ snippet \"forms_search\"}}}\n<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field quickSearchInput\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\" autocorrect=\"off\">\n            <a class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" data-drop-down-close role=\"button\">\n                <svg><use xlink:href=\"#icon-close\"></use></svg>\n            </a>\n            <button type=\"submit\" class=\"search-button\" type=\"submit\">\n                <div class=\"search-button-icon\">\n                    <svg>\n                        <use xlink:href=\"#icon-search\"></use>\n                    </svg>\n                </div>\n            </button>\n        </div>\n    </fieldset>\n</form>\n<section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n"
}
