{
  "components/f/auth/login-form": "<form class=\"flex-component__mobileUp-3 login-form login-form-standard {{#if from_home }}login-form--home {{#if theme_settings.carousel-type \"===\" \"storyboard\"}}login-form--order-{{#if carousel.slides.length \"===\" 1}}1{{else}}{{#if carousel.slides.length \"===\" 3}}1{{else}}2{{/if}}{{/if}}{{else}}login-form--carousel{{/if}}{{else}}login-form-account{{/if}} form\" action=\"{{urls.auth.check_login}}\" method=\"post\">\n    {{inject 'useValidEmail' (lang 'forms.validate.common.email_address')}}\n    {{inject 'enterPass' (lang 'forms.validate.common.password')}}\n    <h3>{{lang 'login.heading'}}</h3>\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"login_email\">{{lang 'login.field_email'}}</label>\n        <input class=\"form-input\" name=\"login_email\" id=\"login_email\" type=\"email\">\n    </div>\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"login_pass\">{{lang 'login.field_password'}}</label>\n        <input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\">\n    </div>\n    <div class=\"form-actions\"><input type=\"submit\" class=\"button button--primary login-form__button\" value=\"{{lang 'login.submit_value' }}\">\n            <div class=\"login-form__links\">\n                <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password'}}</a>\n                {{#if from_home }}\n                      <a class=\"login-form__create-account\" href=\"{{urls.auth.create_account}}\">{{lang 'login.create_account'}}</a>\n                {{/if}}\n            </div>\n\n    </div>\n</form>\n"
}
