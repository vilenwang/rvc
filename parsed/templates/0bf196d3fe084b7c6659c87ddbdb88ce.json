{
  "components/common/wishlist-dropdown": "<form\n  action=\"{{product.add_to_wishlist_url}}\"\n  class=\"form form-wishlist form-action\"\n  data-wishlist-add\n  method=\"post\"\n>\n  <a\n    aria-controls=\"wishlist-dropdown\"\n    aria-expanded=\"false\"\n    class=\"button dropdown-menu-button\"\n    data-dropdown=\"product-wishlist-dropdown\"\n  >\n    <span>{{lang 'account.wishlists.add_item'}}</span>\n  </a>\n  <ul\n    aria-hidden=\"true\"\n    class=\"dropdown-menu\"\n    data-dropdown-content\n    id=\"product-wishlist-dropdown\"\n    tabindex=\"-1\"\n  >\n    {{#if customer.wishlists}} {{#each customer.wishlists}}\n    <li>\n      <input\n        class=\"button {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\"\n        formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\n        type=\"submit\"\n        value=\"{{this.name}}\"\n      />\n    </li>\n    {{/each}} {{else}}\n    <li>\n      <input\n        class=\"button\"\n        type=\"submit\"\n        value=\"{{lang 'account.wishlists.add_to_default'}}\"\n      />\n    </li>\n    {{/if}}\n    <li>\n      <a\n        data-wishlist\n        class=\"button\"\n        href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\"\n        >{{lang 'account.wishlists.create_new'}}</a\n      >\n    </li>\n  </ul>\n</form>\n"
}
