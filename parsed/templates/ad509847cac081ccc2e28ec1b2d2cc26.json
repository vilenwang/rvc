{
  "components/f/header/top-utilities": "<nav class=\"navUser sticky\">\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n        <li class=\"navUser-item navUser-item--store-credit store-credit\">\n            <a class=\"navUser-action navUser-action--storeCredit\"\n               data-dropdown=\"storeCredit-dropdown\"\n               data-options=\"align:bottom\"\n               href=\"{{urls.cart}}\">{{lang 'header.store_credit'}}<span class=\"store-credit__amount\"> {{customer.store_credit.formatted}}</span></a>\n            <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n            </div>\n        </li>\n        {{/if}}\n\n        {{#if settings.gift_certificates_enabled}}\n        <li class=\"navUser-item navUser-item--gift-certificate\">\n            <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\"><span class=\"top-utilities-icon\"><svg><use xlink:href=\"#icon-gift\"></use></svg></span><span class=\"navUser-text\"> {{lang 'common.gift_cert'}}</span></a>\n        </li>\n        {{/if}}\n\n        <li class=\"navUser-item navUser-item--account\">\n            <a class=\"navUser-action {{#if customer}}has-dropdown{{/if}}\" href=\"{{urls.account.index}}\" {{#if customer}}data-dropdown=\"accountOptions\" aria-controls=\"accountOptions\" aria-expanded=\"false\" data-account-menu{{/if}}><span class=\"top-utilities-icon\"><svg><use xlink:href=\"#icon-user\"></use></svg></span><span class=\"navUser-text\"> {{lang 'common.account'}}</span> {{#if customer}}<span class=\"navUser-dropdown-arrow\"><span class=\"top-utilities-icon top-utilities-icon--arrow\"><svg><use xlink:href=\"#icon-chevron-down\"></use></svg></span></span>{{/if}}</a>\n            {{#if customer}}\n            <ul class=\"dropdown-menu dropdown-menu--arrow-right is-open f-open-dropdown\" id=\"accountOptions\" data-dropdown-content=\"\" aria-hidden=\"false\" tabindex=\"-1\">\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                </li>\n                {{#if settings.returns_enabled}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                </li>\n                {{/if}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.inbox}}\">{{lang 'header.messages'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n                </li>\n                {{#if settings.show_payment_methods}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\n                </li>\n                {{/if}}\n                {{#if settings.show_wishlist}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.wishlists.all}}\">{{lang 'header.wishlists.no_name'}}</a>\n                </li>\n                {{/if}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n                </li>\n            </ul>\n            {{/if}}\n        </li>\n\n        {{> components/f/header/header-cart}}\n    </ul>\n</nav>\n",
  "components/f/header/header-cart": "<li class=\"navUser-item navUser-item--cart\">\n    <a\n        class=\"navUser-action\"\n        data-cart-preview\n        data-dropdown=\"cart-preview-dropdown\"\n        data-options=\"align:right\"\n        href=\"{{urls.cart}}\">\n        <span class=\"navUser-item-cartLabel\">{{#if theme_settings.replace_utility_menu_with_icons}}<span class=\"top-utilities-icon\"><svg><use xlink:href=\"#icon-shopping-{{theme_settings.cart_icon}}\"></use></svg></span>{{else}}<span class=\"top-utilities-icon\"><svg><use xlink:href=\"#icon-shopping-{{theme_settings.cart_icon}}\"></use></svg></span><span class=\"cartLabel\"></span>{{/if}}</span> <span class=\"countPill countPill--positive cart-quantity\">{{cart.quantity}}</span>\n    </a>\n\n    <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-autoclose=\"false\" aria-hidden=\"true\">\n        <div class=\"side-cart side-cart-container\">\n            <div class=\"side-cart-title\">{{lang 'b2b.side_cart.cart'}}</div>\n            <div class=\"side-cart-content\"></div>\n            <div class=\"side-cart-details\"></div>\n            <div class=\"side-cart-actions\"></div>\n        </div>\n    </div>\n</li>\n"
}
