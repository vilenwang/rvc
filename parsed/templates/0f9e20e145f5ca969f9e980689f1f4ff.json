{
  "components/f/b2b/quick-add-form": "{{inject 'addSuccess' (lang 'b2b.side_cart.success')}}\n\n<div class=\"sku-add sku-add--sku\">\n  <form\n    class=\"form js-quick-add\"\n    action=\"{{urls.cart}}\"\n    method=\"post\"\n    enctype=\"multipart/form-data\"\n    data-error=\"{{lang 'b2b.side_cart.popup_messages.sku_error'}}\"\n  >\n    <div class=\"add-sku-item\"></div>\n    <fieldset class=\"form-fieldset\">\n      <div class=\"form-field\">\n        <div class=\"form-input-wrap\">\n          <input\n            class=\"form-input form-input--sku-num\"\n            name=\"sku\"\n            type=\"text\"\n            placeholder=\"{{lang 'b2b.header.sku_text'}}\"\n            data-sku\n            autocorrect=\"off\"\n          />\n          <input\n            class=\"form-input form-input--sku-qty\"\n            name=\"qty\"\n            type=\"tel\"\n            value=\"1\"\n            min=\"1\"\n            data-qty\n            autocomplete=\"off\"\n          />\n        </div>\n        <span class=\"quick-add-result\"></span>\n        <button type=\"submit\" class=\"search-button\" type=\"submit\">\n          {{lang 'b2b.header.sku_button'}}\n        </button>\n      </div>\n    </fieldset>\n  </form>\n</div>\n"
}
