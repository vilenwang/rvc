{
  "components/faceted-search/facets/range": "<div class=\"accordion-block\">\n  <div\n    class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\n    role=\"button\"\n    data-collapsible=\"#facetedSearch-content--{{chop (dashcase facet)}}\"\n  >\n    <h5 class=\"accordion-title\">{{ title }}</h5>\n\n    <div class=\"accordion-navigation-actions\">\n      {{#if selected}}\n      <a\n        href=\"{{ remove_url }}\"\n        class=\"facetedSearch-clearLink\"\n        data-faceted-search-facet\n        >{{lang 'search.faceted.clear'}}</a\n      >\n      {{/if}}\n      <svg\n        class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\"\n      >\n        <use xlink:href=\"#icon-add\" />\n      </svg>\n      <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\n        <use xlink:href=\"#icon-remove\" />\n      </svg>\n    </div>\n  </div>\n\n  <div\n    id=\"facetedSearch-content--{{chop (dashcase facet)}}\"\n    class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\"\n  >\n    <form\n      id=\"facet-range-form\"\n      class=\"form facet-range-form\"\n      method=\"get\"\n      data-faceted-search-range\n      novalidate\n    >\n      {{#each current_selected_items}}\n      <input\n        type=\"hidden\"\n        name=\"{{ param_name }}[]\"\n        value=\"{{ param_value }}\"\n      />\n      {{/each}}\n      <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\" />\n      {{#if this.sort}}\n      <input type=\"hidden\" name=\"sort\" value=\"{{this.sort}}\" />\n      {{/if}}\n      <fieldset class=\"form-fieldset\">\n        <div class=\"form-minMaxRow\">\n          <div class=\"form-field\">\n            <input\n              name=\"min_price\"\n              placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\n              min=\"0\"\n              class=\"form-input-min form-input form-input--small\"\n              required\n              type=\"tel\"\n              pattern=\"[0-9]*\"\n              value=\"{{ min_price }}\"\n            />\n          </div>\n\n          <div class=\"form-field\">\n            <input\n              name=\"max_price\"\n              placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\n              min=\"0\"\n              class=\"form-input-max form-input form-input--small\"\n              required\n              type=\"tel\"\n              pattern=\"[0-9]*\"\n              value=\"{{ max_price }}\"\n            />\n          </div>\n\n          <div class=\"form-field\">\n            <button\n              class=\"button button--primary price-range-button\"\n              type=\"submit\"\n            >\n              {{lang 'search.faceted.range.update'}}\n            </button>\n          </div>\n        </div>\n\n        <div class=\"form-inlineMessage\"></div>\n      </fieldset>\n    </form>\n  </div>\n</div>\n"
}
