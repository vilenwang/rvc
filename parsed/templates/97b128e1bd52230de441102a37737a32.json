{
  "components/account/messages-list": "<ul class=\"account-list account-list--messages\">\n    {{#each messages}}\n        <li class=\"account-listItem account-message{{#unless is_reply}} sent-message{{/unless}}\">\n            {{#unless is_reply}}\n                <div class=\"account-message-avatar-container\">\n                    <div class=\"account-message-avatar\">\n                        <div class=\"account-message-avatar-icon\">\n                            <svg aria-hidden=\"true\">\n                                <use xlink:href=\"#icon-user\"></use>\n                            </svg>\n                        </div>\n                    </div>\n                </div>\n            {{/unless}}\n            <div class=\"message-box{{#if is_reply}} is-reply{{/if}}\">\n                <div class=\"account-message-content{{#if is_reply}} is-reply{{/if}}\">\n                    <div class=\"account-returnStatus\">\n                        <span>{{date}}</span>\n                    </div>\n\n                    <h5 class=\"account-product-title{{#if unread}} is-read{{/if}}\">\n                        {{subject}}\n                    </h5>\n                    <p class=\"{{#if unread}}is-read{{/if}}\">\n                        {{{nl2br message}}}\n                    </p>\n                </div>\n            </div>\n            {{#if is_reply}}\n                <div class=\"account-message-avatar-container\">\n                    <div class=\"account-message-avatar\">\n                        <img src=\"{{../../head.favicon}}\">\n                    </div>\n                </div>\n            {{/if}}\n        </li>\n        <div class=\"account-returnStatus-mobile{{#if is_reply}} is-reply{{/if}}\">\n            <span>{{date}}</span>\n        </div>\n    {{/each}}\n</ul>\n"
}
