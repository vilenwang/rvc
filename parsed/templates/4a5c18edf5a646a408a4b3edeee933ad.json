{
  "components/f/b2b/save-cart": "<div class=\"add-to-list__controls\">\n    {{#if customer}}\n    <a href=\"#\" class=\"add-new-list\">{{lang 'b2b.save_cart.add_new_link'}}</a>\n    <div class=\"list-selector\">\n    {{#if customer.wishlists}}\n        <span>{{lang 'b2b.save_cart.save_to_list'}}</span>\n        <select name=\"list-id\" id=\"list-id\" class=\"form-select\">\n            {{#each customer.wishlists}}\n            <option value=\"{{id}}\">{{name}} ({{num_items}})</option>\n            {{/each}}\n        </select>\n    {{/if}}\n    </div>\n    <input type=\"button\" data-save-cart class=\"button button__save-cart\" value=\"{{lang 'b2b.save_cart.button_text'}}\" />\n    {{else}}\n    <a href=\"{{urls.auth.login}}\">{{lang 'b2b.save_cart.sign_in'}}</a>\n    {{/if}}\n</div>\n"
}
