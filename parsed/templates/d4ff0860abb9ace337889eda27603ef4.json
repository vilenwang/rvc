{
  "components/f/auth/login-form-email": "{{inject 'passwordlessRedirectUrl' theme_settings.login_redirect_url}}\n<form class=\"flex-component__mobileUp-3 login-form login-form-email {{#if from_home }}login-form--home {{#if theme_settings.carousel-type \"===\" \"storyboard\"}}login-form--order-{{#if carousel.slides.length \"===\" 1}}1{{else}}{{#if carousel.slides.length \"===\" 3}}1{{else}}2{{/if}}{{/if}}{{else}}login-form--carousel{{/if}}{{else}}login-form-account{{/if}} form\" action=\"{{urls.auth.check_login}}\" method=\"post\">\n    {{inject 'useValidEmail' (lang 'forms.validate.common.email_address')}}\n    {{inject 'enterPass' (lang 'forms.validate.common.password')}}\n    <div class=\"login-email-sent\">\n        <span>{{lang 'login.email_link_sent'}}</span>\n        <div class=\"login-form__links\">\n            <a class=\"login-email-redo\" href=\"#\">{{lang 'login.use_another_email'}}</a>\n        </div>\n    </div>\n    <h3>{{lang 'login.heading'}}</h3>\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"email\">{{lang 'login.field_email'}}</label>\n        <input class=\"form-input\" name=\"email\" id=\"login_email\" type=\"email\">\n    </div>\n    <div class=\"form-actions\">\n        <input type=\"submit\" class=\"button button--primary login-form__button\" value=\"{{lang 'login.email_a_link' }}\">\n        <div class=\"login-form__links\">\n            {{#if from_home }}\n                  <a class=\"login-form__create-account\" href=\"{{urls.auth.create_account}}\">{{lang 'login.create_account'}}</a>\n            {{/if}}\n        </div>\n    </div>\n</form>\n"
}
