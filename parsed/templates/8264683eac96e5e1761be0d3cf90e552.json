{
  "components/f/products/price-range": "<div class=\"price-section  rrp-price--withTax rrp-price--withoutTax\"{{#or price.retail_price_range.min.with_tax price.retail_price_range.max.with_tax price.retail_price_range.min.without_tax price.retail_price_range.max.without_tax}}{{else}} style=\"display: none;\"{{/or}}>\n    <div class=\"price-section--label\">\n        {{theme_settings.pdp-retail-price-label}}\n    </div>\n\n    <div class=\"price-section--wrapper\">\n        {{#and price.retail_price_range.min.with_tax price.retail_price_range.max.with_tax}}\n            <div class=\"price-section price-section--withTax rrp-price--withTax{{#if price.without_tax}}{{#if theme_settings.MRRP-primary-price '==' 'with_tax'}} price-section--primary{{else}} price--hidden{{/if}}{{/if}}\">\n                <span data-product-rrp-price-with-tax class=\"price price--rrp\">{{price.retail_price_range.min.with_tax.formatted}} - {{price.retail_price_range.max.with_tax.formatted}}</span>\n            </div>\n        {{else}}\n            {{#if price.with_tax}}\n                <div class=\"price-section--withTax rrp-price--withTax{{#if price.without_tax}}{{#if theme_settings.MRRP-primary-price '==' 'with_tax'}} price-section--primary{{else}} price--hidden{{/if}}{{/if}}\" style=\"display: none;\">\n                    <span data-product-rrp-with-tax class=\"price price--rrp\">\n                        {{price.rrp_with_tax.formatted}}\n                    </span>\n                </div>\n            {{/if}}\n        {{/and}}\n        {{#and price.retail_price_range.min.without_tax price.retail_price_range.max.without_tax}}\n            <div class=\"price-section price-section--withoutTax rrp-price--withoutTax{{#if price.with_tax}}{{#if theme_settings.MRRP-primary-price '==' 'without_tax'}} price-section--primary{{else}} price--hidden{{/if}}{{/if}}\">\n                <span data-product-rrp-price-without-tax class=\"price price--rrp\">{{price.retail_price_range.min.without_tax.formatted}} - {{price.retail_price_range.max.without_tax.formatted}}</span>\n            </div>\n        {{else}}\n            {{#if price.without_tax}}\n                <div class=\"price-section--withoutTax rrp-price--withoutTax{{#if price.with_tax}}{{#if theme_settings.MRRP-primary-price '==' 'without_tax'}} price-section--primary{{else}} price--hidden{{/if}}{{/if}}\" style=\"display: none;\">\n                    <span data-product-rrp-price-without-tax class=\"price price--rrp\">\n                        {{price.rrp_without_tax.formatted}}\n                    </span>\n                </div>\n            {{/if}}\n        {{/and}}\n    </div>\n</div>\n{{!-- Never display the \"non-sales price\" if there is a price range to be shown, but we do want the element on the page --}}\n<div class=\"price-section non-sale-price--withTax non-sale-price--withoutTax\" style=\"display: none;\">\n    <div class=\"price-section--label price--sale-label\">\n        {{theme_settings.pdp-non-sale-price-label}}\n    </div>\n\n    <div class=\"price-section--wrapper\">\n        {{#and price.price_range.min.with_tax price.price_range.max.with_tax}}\n            <div class=\"price-section--withTax non-sale-price--withTax{{#if price.without_tax}}{{#if theme_settings.primary-price '==' 'with_tax'}} price-section--primary{{else}} price--hidden{{/if}}{{/if}}\" style=\"display: none;\">\n                <span data-product-non-sale-price-with-tax class=\"price price--non-sale\">\n                    {{price.non_sale_price_with_tax.formatted}}\n                </span>\n            </div>\n        {{/and}}\n        {{#and price.price_range.min.without_tax price.price_range.max.without_tax}}\n            <div class=\"price-section--withoutTax non-sale-price--withoutTax{{#if price.with_tax}}{{#if theme_settings.primary-price '==' 'without_tax'}} price-section--primary{{else}} price--hidden{{/if}}{{/if}}\" style=\"display: none;\">\n                <span data-product-non-sale-price-without-tax class=\"price price--non-sale\">\n                    {{price.non_sale_price_without_tax.formatted}}\n                </span>\n            </div>\n        {{/and}}\n    </div>\n</div>\n\n<div class=\"price-section\">\n    <div class=\"price-section--label\" style=\"display: none;\">\n        <span class=\"price-label\" >\n            {{theme_settings.pdp-price-label}}\n        </span>\n        <span class=\"price-now-label\">\n            {{theme_settings.pdp-sale-price-label}}\n        </span>\n    </div>\n    <div class=\"price-section--wrapper\">\n        {{#and price.price_range.min.with_tax price.price_range.max.with_tax}}\n            <div class=\"price-section--withTax{{#and price.price_range.min.without_tax price.price_range.max.without_tax}}{{#if theme_settings.primary-price '==' 'with_tax'}} price-section--primary{{/if}}{{else}} price-section--primary{{/and}}\">\n                <span data-product-price-with-tax class=\"price price--withTax{{#and price.price_range.min.without_tax price.price_range.max.without_tax}}{{#if theme_settings.primary-price '==' 'with_tax'}} price-primary price-primary--priceRange{{/if}}{{else}} price-primary price-primary--priceRange{{/and}}\">{{price.price_range.min.with_tax.formatted}} - {{price.price_range.max.with_tax.formatted}}</span>\n                {{#and price.price_range.min.without_tax price.price_range.max.without_tax}}\n                    <abbr title=\"{{lang 'products.including_tax'}}\">{{lang 'products.price_with_tax' tax_label=price.price_range.min.tax_label}}</abbr>\n                    {{else}}\n                        {{#if theme_settings.show-tax-label}}\n                            <abbr title=\"{{lang 'products.including_tax'}}\">{{lang 'products.price_with_tax' tax_label=price.price_range.min.tax_label}}</abbr>\n                        {{/if}}\n                {{/and}}\n            </div>\n        {{/and}}\n\n        {{#and price.price_range.min.without_tax price.price_range.max.without_tax}}\n            <div class=\"price-section--withoutTax{{#and price.price_range.min.with_tax price.price_range.max.with_tax}}{{#if theme_settings.primary-price '==' 'without_tax'}} price-section--primary{{/if}}{{else}} price-section--primary{{/and}}\">\n                <span data-product-price-without-tax class=\"price price--withoutTax{{#and price.price_range.min.with_tax price.price_range.max.with_tax}}{{#if theme_settings.primary-price '==' 'without_tax'}} price-primary price-primary--priceRange{{/if}}{{else}} price-primary price-primary--priceRange{{/and}}\">{{price.price_range.min.without_tax.formatted}} - {{price.price_range.max.without_tax.formatted}}</span>\n                    {{#and price.price_range.min.with_tax price.price_range.max.with_tax}}\n                        <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.price_range.min.tax_label}}</abbr>\n                    {{else}}\n                        {{#if theme_settings.show-tax-label}}\n                            <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.price_range.min.tax_label}}</abbr>\n                        {{/if}}\n                    {{/and}}\n            </div>\n        {{/and}}\n    </div>\n</div>\n\n{{!-- Never display the \"saving price\" element by default if there is a price range on the page to be shown --}}\n{{#if page_type '===' 'product'}}\n     <div class=\"price-section price-section--saving price\" style=\"display: none;\">\n            <span class=\"price\">{{lang 'products.you_save_opening_text'}}</span>\n            <span data-product-price-saved class=\"price price--saving\">\n                {{price.saved.formatted}}\n            </span>\n     </div>\n{{/if}}\n"
}
