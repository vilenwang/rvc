{
  "components/f/b2b/account-bar": "  <div class=\"account-bar flex-component__mobileUp-3 flex-component__mediumUp-5\">\n    <div class=\"account-bar__item\">\n      <span class=\"account-bar__icon\">\n          <svg>\n              <use xlink:href=\"#icon-user\"></use>\n          </svg>\n      </span>\n      <div class=\"account-bar__text\">\n        <h4>{{lang 'b2b.account_bar.account.header_text'}}{{#if customer.name}}, {{first (split customer.name ' ')}}{{/if}}</h4>\n        <a href=\"{{urls.account.index}}\" class=\"account-bar__link\">{{lang 'b2b.account_bar.account.link_text'}}</a>\n      </div>\n    </div>\n    <div class=\"account-bar__item\">\n      <span class=\"account-bar__icon\">\n          <svg>\n              <use xlink:href=\"#icon-history\"></use>\n          </svg>\n      </span>\n      <div class=\"account-bar__text\">\n        <h4>{{lang 'b2b.account_bar.orders.header_text'}}</h4>\n        <a href=\"{{urls.account.orders.all}}\" class=\"account-bar__link\">{{lang 'b2b.account_bar.orders.link_text'}}</a>\n      </div>\n    </div>\n    {{#and theme_settings.show_phone_number_in_header settings.phone_number}}\n    <div class=\"account-bar__item\">\n      <span class=\"account-bar__icon\">\n          <svg>\n              <use xlink:href=\"#icon-phone\"></use>\n          </svg>\n      </span>\n      <div class=\"account-bar__text\">\n        <h4>{{lang 'b2b.account_bar.call_us.header_text'}}</h4>\n        <a href=\"tel:{{settings.phone_number}}\" class=\"account-bar__link\">{{lang 'b2b.account_bar.call_us.link_text'}} {{settings.phone_number}}</a>\n      </div>\n    </div>\n    {{/and}}\n    <div class=\"account-bar__item\">\n      <span class=\"account-bar__icon\">\n          <svg>\n              <use xlink:href=\"#icon-envelope\"></use>\n          </svg>\n      </span>\n      <div class=\"account-bar__text\">\n        <h4>{{lang 'b2b.account_bar.messages.header_text'}}</h4>\n        <a href=\"{{urls.account.inbox}}\" class=\"account-bar__link\">{{lang 'b2b.account_bar.messages.link_text'}}</a>\n      </div>\n    </div>\n    {{#if settings.returns_enabled}}\n    <div class=\"account-bar__item\">\n      <span class=\"account-bar__icon\">\n          <svg>\n              <use xlink:href=\"#icon-reply\"></use>\n          </svg>\n      </span>\n      <div class=\"account-bar__text\">\n        <h4>{{lang 'b2b.account_bar.returns.header_text'}}</h4>\n        <a href=\"{{urls.account.returns}}\" class=\"account-bar__link\">{{lang 'b2b.account_bar.returns.link_text'}}</a>\n      </div>\n    </div>\n    {{/if}}\n  </div>\n"
}
